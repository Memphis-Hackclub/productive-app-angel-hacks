{
    "name": "FakechromeExtensions",
    "version":"1.0", 
    "manifest_version": 2,
    "content_scripts":[
      {
        "matches": ["*://www.youtube.com/"],
        "js": ["popup.js"]
      }
    ],
    "browser_action":{
      "default_popup": "popup.html",
      "defult_title": "Popup"
    },
    "permissions": [
      "tabs"
   ]
  }
